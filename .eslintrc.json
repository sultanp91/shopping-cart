{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "only-warn"],
  "rules": {
    "react/jsx-filename-extension": [0],
    "import/extensions": "off",
    "import/prefer-default-export": "off"
  }
}
